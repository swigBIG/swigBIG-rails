<% unless @user_swig_feed.blank? %>
  <% if @user_swig_feed.verb.eql?("loyalty reward") %>
    <% unless @user_swig_feed.actor.name.blank? %>
      <%= "Live Swig Feed: #{@user_swig_feed.actor.name rescue @user_swig_feed.actor.email} got loyalty reward at " %><%= link_to @user_swig_feed.object.name, bar_profile_path(@user_swig_feed.object) %>
    <% else %>
      <%= "Live Swig Feed: #{@user_swig_feed.actor.email rescue @user_swig_feed.actor.email} got loyalty reward at " %><%= link_to @user_swig_feed.object.name, bar_profile_path(@user_swig_feed.object) %>
    <% end %>
  <% elsif @user_swig_feed.verb.eql?("popularity reward") %>
    <% unless @user_swig_feed.actor.name.blank? %>
      <%= "Live Swig Feed: #{@user_swig_feed_actor.name rescue @user_swig_feed.actor.email} got popularity reward at " %><%= link_to @user_swig_feed_object.name, bar_profile_path(@user_swig_feed_object) %>
    <% else %>
      <%= "Live Swig Feed: #{@user_swig_feed_actor.email rescue @user_swig_feed.actor.email} got popularity reward at " %><%= link_to @user_swig_feed_object.name, bar_profile_path(@user_swig_feed_object) %>
    <% end %>
  <% elsif @user_swig_feed.verb.eql?("bigswig unlock") %>
    <%= "Live Swig Feed:  Unlocked Bigswig at" %>&nbsp;<%= link_to @user_swig_feed.object.name, bar_profile_path(@user_swig_feed.object) %>
  <% elsif @user_swig_feed.verb.eql?("user swiging") %>
    <% unless @user_swig_feed.actor.name.blank? %>
      <%= "Live Swig Feed: #{@user_swig_feed.actor.name rescue @user_swig_feed.actor.email} swiging at  "%>&nbsp;<%= link_to @user_swig_feed.object.name, bar_profile_path(@user_swig_feed.object) %>
    <% else %>
      <%= "Live Swig Feed: #{@user_swig_feed.actor.email rescue @user_swig_feed.actor.email} swiging at  "%>&nbsp;<%= link_to @user_swig_feed.object.name, bar_profile_path(@user_swig_feed.object) %>
    <% end %>
  <% end %>
<% else %>
  <%= "Live Swig Feed: No users feed yet!".html_safe %>
<% end %>