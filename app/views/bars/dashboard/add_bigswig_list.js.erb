var days = ["monday", "tuesday", "wednesday", "thursday", "friday", "saturday", "sunday"];

for(var i = 0; i <= 6; i++)
  for(var j = 1; j <= 4; j++)
    if(j != 4)
      $("#"+days[i]+"_bigswig_lists"+j).html('<%=j(render partial: "bigswig_list") %>');
    else
      $("#"+days[i]+"_notice_lists"+j).text('Add Success!');
