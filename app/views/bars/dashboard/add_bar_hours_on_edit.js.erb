$("#bar_hour_list").append("<%=j (render partial: 'shared/bar_hours_add_partial', locals: {count: @counter.to_i + 1} ) %>");

$("#first_day_<%= @counter.to_i + 1 %> [value='"+(parseInt($("#last_day<%= params[:count] %>").val()) + 1)+"']").attr('selected',true);

$("#last_day<%= @counter.to_i + 1 %> [value='"+(parseInt($("#last_day<%= params[:count] %>").val()) + 1)+"']").attr('selected',true);

$('#bigswig_link_on_update').html("<%=j link_to "Add", bars_add_bar_hours_on_edit_path(count: @counter.to_i + 1), method: :post, remote: true, class: "btn", style: "color: green:" %>");

$('#bigswig_link_on_update').append("<%=j hidden_field_tag "total", @counter.to_i + 1 %>");
